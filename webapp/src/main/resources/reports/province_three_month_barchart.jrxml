	<queryString>
		<![CDATA[SELECT
`clinic_code`,
`clinic_short_name`,
`sub_district_name`,
`sub_district_short_name`,
`district_name`,
`district_short_name`,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `staff_attitude_rating` else 0 end), 2) as staff_attitude_rating_onem,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `cleanliness_rating` else 0 end), 2) as cleanliness_rating_onem,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `waiting_times_rating` else 0 end), 2) as waiting_times_rating_onem,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `drug_availability_rating` else 0 end), 2) as drug_availability_rating_onem,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `infection_control_rating` else 0 end), 2) as infection_control_rating_onem,
round(AVG( case when submission_date BETWEEN $P{first_first_month} AND $P{end_date}  then `safe_and_secure_care_rating` else 0 end), 2) as safe_and_secure_care_rating_onem,

round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `staff_attitude_rating` else 0 end), 2) as staff_attitude_rating_twom,
round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `cleanliness_rating` else 0 end), 2) as cleanliness_rating_twom,
round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `waiting_times_rating` else 0 end), 2) as waiting_times_rating_twom,
round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `drug_availability_rating` else 0 end), 2) as drug_availability_rating_twom,
round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `infection_control_rating` else 0 end), 2) as infection_control_rating_twom,
round(AVG( case when submission_date BETWEEN $P{first_second_month} AND $P{end_date}  then `safe_and_secure_care_rating` else 0 end), 2) as safe_and_secure_care_rating_twom,

round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `staff_attitude_rating` else 0 end), 2) as staff_attitude_rating_threem,
round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `cleanliness_rating` else 0 end), 2) as cleanliness_rating_threem,
round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `waiting_times_rating` else 0 end), 2) as waiting_times_rating_threem,
round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `drug_availability_rating` else 0 end), 2) as drug_availability_rating_threem,
round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `infection_control_rating` else 0 end), 2) as infection_control_rating_threem,
round(AVG( case when submission_date BETWEEN $P{first_three_month} AND $P{end_date}  then `safe_and_secure_care_rating` else 0 end), 2) as safe_and_secure_care_rating_threem
 FROM `data_mart_rating`]]>
